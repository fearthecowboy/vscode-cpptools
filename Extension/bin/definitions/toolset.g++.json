{
  "name": "GNU C++ [x64]",
  "import": [
    "common/gcc.json",
    "common/defaults.json"
  ],
  "discover": {
    "binary": "g++",
    "match": {
      "^(?<ver>\\d+[.]\\d+[.]\\d+)(?:-\\S*)?$" : { "version": "${ver}" }
    }
  },
  "intellisense": {
    "compiler": "gcc",
    "architecture": "x64",
    "bits": 64,
  }
}